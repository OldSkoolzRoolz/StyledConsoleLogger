{
    "Logging": {
        "LogLevel": {
            "Default": "Trace",
            "Microsoft": "Trace"
        },
        "Console": {
            "LogLevel": {
                "Default": "Trace",
                "Microsoft": "Trace"
            },
            "FormatterName": "StyledLoggerFormatter",
            "FormatterOptions": {
                "CustomPrefix": "|-<[",
                "CustomSuffix": "]>-|",
                "SingleLine": true,
                "IncludeScopes": true,
                "TimestampFormat": "HH:mm:ss",
                "UseUtcTimestamp": true
            }
        }
    },
    "AllowedHosts": "*"
}